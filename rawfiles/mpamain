package main

import (
	"{{.}}/src/configuration"
	"{{.}}/src/controllers"

	"github.com/deltegui/phoenix"
)

func main() {
	app := phoenix.NewApp()
	app.Configure().
		SetProjectInfo("{{.}}", "0.1.0").
		EnableLogoFile().
		EnableStaticServer().
		EnableSessions()
	config := configuration.Load()
	controllers.Register(app)
	app.Run(config.ListenURL)
}
