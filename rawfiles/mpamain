package main

import (
	"{{.}}/src/configuration"
	"{{.}}/src/controllers"

	"github.com/deltegui/phoenix"
)

func main() {
	phoenix.Configure().
		SetProjectInfo("{{.}}", "0.1.0").
		EnableLogoFile().
		EnableStaticServer().
		EnableTemplates()
	config := configuration.Load()
	controllers.Register()
	phoenix.Run(config.ListenURL)
}
